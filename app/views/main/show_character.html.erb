<div class="container">
  <div class="row">
    <img id="callboard-logo" class="col-md-6 col-md-push-3 col-xs-12" src="/assets/logos/CALLBOARDLOGO.png" alt="callboard-logo" />
  </div>
  <div class="row quote-box">
    <h2><%= @character.name %></h2>
    <div class="col-md-3 profile-pic">
      <img src="/assets/callboys-profile-pictures/<%= @character.name %>.png" alt="<%= @character.name %>" />
    </div>
    <div class="col-md-9 quote-list">
      <ul>
        <% @character.quotes.each do |quote| %>
          <li><%= link_to quote.text, generator_path(quote) %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
